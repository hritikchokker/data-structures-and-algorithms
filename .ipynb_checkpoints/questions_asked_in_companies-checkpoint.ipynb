    "# Factorial Of a number"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "class Solution:\n",
    "    def factorial (self, N):\n",
    "        fact=1\n",
    "        for i in range(2,N+1):\n",
    "            fact=fact*i\n",
    "        return fact\n",
    "sol = Solution()\n",
    "print(sol.factorial(10))"
   ]
  },
